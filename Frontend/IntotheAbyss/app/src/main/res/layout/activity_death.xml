<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.DeathActivity" android:id="@+id/coordinatorLayout">

    <android.support.design.widget.AppBarLayout
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:theme="@style/AppTheme.NoActionBar.AppBarOverlay"
    />

    <include android:id="@+id/include"
             android:layout_height="wrap_content"
             android:layout_width="wrap_content" tools:layout_editor_absoluteY="0dp"
             tools:layout_editor_absoluteX="0dp"/>

    <Button
            android:text="Go Home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/homeButton"
            tools:layout_editor_absoluteY="538dp" tools:layout_editor_absoluteX="179dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
    />

</android.support.constraint.ConstraintLayout>